{
  "name": "coc-quicktype",
  "version": "0.0.3",
  "description": "Generate strongly typed models or serializers through JSON",
  "author": "chuyanlong <a1008611abcd@126.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.82"
  },
  "scripts": {
    "watch": "node esbuild.mjs --watch",
    "build": "node esbuild.mjs",
    "prepare": "node esbuild.mjs"
  },
  "devDependencies": {
    "coc.nvim": "^0.0.83-next.18",
    "esbuild": "^0.19.8",
    "typescript": "^5.3.3"
  },
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:typescript",
    "onLanguage:java",
    "onLanguage:go",
    "onLanguage:swift",
    "onLanguage:dart",
    "onLanguage:cpp",
    "onLanguage:javascript",
    "onLanguage:php",
    "onLanguage:rust",
    "onLanguage:cs",
    "onLanguage:elm"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-quicktype configuration",
      "properties": {
        "coc-quicktype.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-quicktype extension"
        }
      }
    },
    "commands": [
      {
        "command": "quicktype.generate",
        "title": "Convert the JSON data in the clipboard into a model or sequencer"
      }
    ]
  },
  "dependencies": {
    "quicktype-core": "^23.2.6"
  }
}
